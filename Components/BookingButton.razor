@inject IDialogService DialogService


<MudButton @onclick="OpenDialog"Variant="Variant.Outlined" Color="Color.Info" Size="Size.Medium">
    Book Now
</MudButton>


@code {

    private void OpenDialog()
    {
		var options = new DialogOptions { CloseOnEscapeKey = true };
        DialogService.Show<BookingDialog>("Booking Form", options);
    }
}